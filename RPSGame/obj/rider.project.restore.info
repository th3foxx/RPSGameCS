# This program generates a large random number
import random

def generate_large_number():
    # Generate a random integer with 20 digits
    large_number = random.randint(10**19, 10**20 - 1)
    return large_number

if __name__ == "__main__":
    # Generate and print the large number
    large_number = generate_large_number()
    print(f"The large number is: {large_number}")
